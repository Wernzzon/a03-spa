    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-jsdoc": "^37.0.3",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.1.1",
    "eslint-plugin-standard": "^4.0.1",
    "htmlhint": "^1.1.2",
    "snowpack": "^3.8.8",
    "standard": "*",
    "stylelint": "^14.5.1"
  },
  "dependencies": {
    "uuid": "^8.3.2"
